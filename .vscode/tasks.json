{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "Build with Makefile",
		"type": "shell",
		"command": "/usr/bin/make",
		"args": ["all"],
		"options": {
		  "cwd": "${workspaceFolder}"
		},
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"problemMatcher": [
		  "$gcc"
		],
		"detail": "Run Makefile build"
	  }
	]
  }
